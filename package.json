{
  "name": "gubb",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Jonas RÃ¸ssum",
  "main": "dist/index.js",
  "module": "dist/gubb.esm.js",
  "type": "module",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env CI=true tsdx test",
    "test-integration": "npm run test-integration-esm && npm run test-integration-basic && npm run test-integration-multiple-files",
    "test-integration-esm": "npm run --prefix example/esm test",
    "test-integration-basic": "npm run --prefix example/basic test",
    "test-integration-multiple-files": "npm run --prefix example/multiple-files test",
    "test:watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "release": "np",
    "version": "npm run build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "esm": "^3.2.25",
    "husky": "^3.1.0",
    "np": "^5.2.1",
    "tsdx": "^0.12.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "boxen": "^4.2.0",
    "cardinal": "^2.1.1",
    "chalk": "^3.0.0"
  },
  "description": "gubb is a very minimalistic testing library.",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joglr/gubb.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/joglr/gubb/issues"
  },
  "homepage": "https://github.com/joglr/gubb#readme"
}
